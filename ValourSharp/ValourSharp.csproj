<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<EnablePreviewFeatures>True</EnablePreviewFeatures>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Version>1.0.1</Version>
		<Authors>Coca</Authors>
		<PackageLicenseExpression>AGPL-3.0-only</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<Description>.NET Library for making Valour bots!</Description>
		<PackageReleaseNotes>Adding Logo</PackageReleaseNotes>
		<PackageTags>Valour ValourSharp Bot ValourBot</PackageTags>
		<RepositoryUrl>https://github.com/Coca162/ValourSharp</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageIcon>logo128.png</PackageIcon>
	</PropertyGroup>

	<ItemGroup>
	  <None Include="..\logo128.png">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Valour\Valour\API\Valour.Api.csproj" />
	</ItemGroup>

</Project>